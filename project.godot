config_version=5

[application]

config/name="ParryTheFloor"
run/main_scene="res://UI/MainMenu/MainMenu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="ParryTheFloorSave"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/image="res://Sprites/LoadingScreens/splash.png"
boot_splash/use_filter=false
config/icon="res://Sprites/LoadingScreens/perry_smug.png"
boot_splash/minimum_display_time=1000
config/minimum_display_time=2000

[audio]

buses/default_bus_layout="res://Audio/default_bus_layout.tres"

[autoload]

Music="*res://Scripts/Audio/AudioStreamPlayer.gd"
Signals="*res://Scripts/Global/Signals.gd"
LevelData="*res://Scripts/Global/LevelData.gd"
SaveLoad="*res://Scripts/Global/SaveLoad.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="viewport"
window/dpi/allow_hidpi=false

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/nklbdev.aseprite_importers/plugin.cfg")

